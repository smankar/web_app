<% provide(:title, "Users") %>
<h1>Upload File</h1>
<h6>1</h6>

<div class="container">
  <div class="row">
    <div class="col-xs-12">


      <%= form_tag import_uploads_path, multipart: true do %>
          <%= file_field_tag :file, class: "btn btn-lg btn-primary" %>
          <%= submit_tag "Import", class: "btn btn-lg btn-primary" %>
      <% end %>

    </div>
  </div>
</div>